{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "package",
            "package": {
                "name": "components/highlightjs",
                "version": "9.7.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/components/highlightjs/archive/9.7.0.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "components/highlightjs": "9.7.*",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.6.5",
        "drupal/ace_editor": "^1.0",
        "drupal/admin_toolbar": "^3.0",
        "drupal/adminimal_admin_toolbar": "^1.11",
        "drupal/anchor_link": "^1.6",
        "drupal/better_exposed_filters": "^5.0@beta",
        "drupal/bootstrap4": "^2.1",
        "drupal/bootstrap_barrio": "^5.1",
        "drupal/captcha": "^1.1",
        "drupal/ckeditor_media_embed": "^1.3",
        "drupal/content_lock": "^2.2",
        "drupal/core-composer-scaffold": "^9.1",
        "drupal/core-project-message": "^9.1",
        "drupal/core-recommended": "^9.1",
        "drupal/ctools": "^3.4",
        "drupal/devel": "^4.1",
        "drupal/devel_kint_extras": "^1.0",
        "drupal/diff": "^1.0",
        "drupal/dropzonejs": "^2.5",
        "drupal/drupal-library-installer-plugin": "^0.3.0",
        "drupal/ds": "^3.10",
        "drupal/easy_breadcrumb": "^1.14",
        "drupal/entity": "^1.1",
        "drupal/entity_browser": "^2.5",
        "drupal/facets": "^1.7",
        "drupal/flag": "4.x-dev",
        "drupal/focal_point": "^1.5",
        "drupal/google_tag": "^1.4",
        "drupal/graphql": "^4",
        "drupal/honeypot": "^2.0",
        "drupal/hreflang": "^1.4",
        "drupal/htmlmail": "^3.0@alpha",
        "drupal/image_effects": "^3.0",
        "drupal/jquery_ui": "^1.4",
        "drupal/jquery_ui_checkboxradio": "^1.2",
        "drupal/jquery_ui_datepicker": "^1.0",
        "drupal/jquery_ui_tabs": "^1.1",
        "drupal/jquery_ui_tooltip": "^1.1",
        "drupal/lazy": "^3.6",
        "drupal/link_attributes": "^1.11",
        "drupal/mailsystem": "^4.3",
        "drupal/metatag": "^1.15",
        "drupal/module_filter": "^3.2",
        "drupal/pager_metadata": "^1.0@beta",
        "drupal/paragraphs": "^1.12",
        "drupal/pathauto": "^1.8",
        "drupal/permissions_filter": "^1.2",
        "drupal/rabbit_hole": "^1.0@beta",
        "drupal/redirect": "^1.6",
        "drupal/scheduler": "^1.3",
        "drupal/schema_metatag": "^2.1",
        "drupal/search_api": "^1.19",
        "drupal/search_api_autocomplete": "^1.4",
        "drupal/sitemap": "^2.0@beta",
        "drupal/smtp": "^1.0",
        "drupal/token": "^1.9",
        "drupal/transliterate_filenames": "^1.5",
        "drupal/typed_data": "^1.0@alpha",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drupal/vbo_export": "^3.2",
        "drupal/views_bulk_operations": "^3.10",
        "drupal/views_exposed_filter_blocks": "^1.1",
        "drupal/webform": "^6.0",
        "drupal/xmlsitemap": "^1.0",
        "drupal/yoast_seo": "^2.0@alpha",
        "enyo/dropzone": "^5.7",
        "jpgraph/jpgraph": "^4.0",
        "mpdf/mpdf": "^8.0",
        "npm-asset/ace-builds": "1.3.3",
        "npm-asset/blazy": "^1.8",
        "npm-asset/dropzone": "^4.3.0",
        "npm-asset/slick-carousel": "^1.8",
        "oomphinc/composer-installers-extender": "^1.1",
        "phpoffice/phpspreadsheet": "^1.6",
        "react/promise": "^2.8"
    },
    "require-dev": {
        "drupal/console": "^1.9",
        "drush/drush": "^10.4",
        "kint-php/kint": "^3.3",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "patches": {},
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/slick": [
                "npm-asset/slick-carousel"
            ],
            "web/libraries/ace": [
                "npm-asset/ace-builds"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "./web"
            },
            "file-mapping": {
                "[web-root]/robots.txt": false,
                "[web-root]/.htaccess": false
            },
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        },
        "drupal-libraries": {
            "library-directory": "web/libraries",
            "libraries": [
                {
                    "name": "dropzone",
                    "package": "npm-asset/dropzone"
                },
                {
                    "name": "blazy",
                    "package": "npm-asset/blazy"
                },
                {
                    "name": "slick",
                    "package": "npm-asset/slick-carousel"
                },
                {
                    "name": "ace",
                    "package": "npm-asset/ace-builds"
                }
            ]
        }
    }
}
